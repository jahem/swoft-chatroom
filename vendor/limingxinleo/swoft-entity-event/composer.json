{
    "name": "limingxinleo/swoft-entity-event",
    "type": "library",
    "license": "MIT",
    "keywords": [
        "php",
        "swoole",
        "swoft",
        "db",
        "model"
    ],
    "description": "Swoft实体事件组件",
    "authors": [
        {
            "name": "李铭昕",
            "email": "limingxin@swoft.org"
        }
    ],
    "autoload": {
        "psr-4": {
            "Swoftx\\EntityEvent\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "SwoftTest\\": "tests"
        }
    },
    "require": {
        "php": ">=7.1",
        "swoft/framework": "^1.0.24",
        "swoft/db": "^1.0"
    },
    "require-dev": {
        "swoft/swoole-ide-helper": "dev-master",
        "phpunit/phpunit": "^5.7",
        "phpstan/phpstan": "^0.10.5"
    },
    "extra": {
        "config-plugin": {
            "swoft.components": [
                "config/components.php"
            ]
        }
    }
}
